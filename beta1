    showTab("UpdateLogs")
end)

HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

MainFrame.VisibleChanged:Connect(function()
    updateHideButtonPosition()
end)

--- Inicjalizacja i uruchomienie ---
checkGamePassStatus(true) 
showTab("Home") 
updateHideButtonPosition()

spawn(function()
    task.wait(1)
    sendNotification("Witaj w Universal Scripts! Skrypt załadowany.")
    checkGamePassStatus(false) 
end)

task.wait(0.5)
MainFrame.Visible = true
