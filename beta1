--- Połączenia przycisków Executor ---
ExecuteButton.MouseButton1Click:Connect(function()
    local code = ExecutorTextBox.Text
    if code and #code > 0 and code ~= ExecutorTextBox.PlaceholderText then
        local func, err = loadstring(code)
        if func then
            local success, runErr = pcall(func)
            if success then
                sendNotification("Kod wykonany pomyślnie!")
            else
                sendNotification("Błąd wykonania kodu: " .. tostring(runErr))
                warn("Błąd wykonania kodu z ExecutorTextBox:", runErr)
            end
        else
            sendNotification("Błąd parsowania kodu: " .. tostring(err))
            warn("Błąd loadstring dla ExecutorTextBox:", err)
        end
    else
        sendNotification("Wklej kod do wykonania w polu tekstowym.")
    end
end)

ClearButton.MouseButton1Click:Connect(function()
    ExecutorTextBox.Text = ""
    sendNotification("Pole tekstowe wyczyszczone.")
end)


--- Połączenia zakładek i przycisku Hide ---
