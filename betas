end)

MM2ButtonHome.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vertex-peak/vertex/refs/heads/main/loadstring"))()
    sendNotification("Skrypt MM2", "Skrypt MM2 załadowany!")
end)

-- Połączenia przycisków Premium (z ponownym sprawdzeniem Game Passa)
SpectateButtonPremium.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success and currentHasGamePass then
        loadstring(game:HttpGet("https://pastebin.com/raw/UFVLWQRw"))()
        sendNotification("Narzędzie Obserwacji", "Skrypt obserwacji załadowany!")
    else
        sendNotification("Universal Scripts", "Musisz posiadać Game Pass Premium, aby użyć tej funkcji!")
        if success then -- Tylko jeśli pcall się powiódł, ale gracz nie ma Game Passa
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
        end
    end
end)

MM2ButtonPremium.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success and currentHasGamePass then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/vertex-peak/vertex/refs/heads/main/loadstring"))()
        sendNotification("Skrypt MM2", "Skrypt MM2 załadowany!")
    else
        sendNotification("Universal Scripts", "Musisz posiadać Game Pass Premium, aby użyć tej funkcji!")
        if success then
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
        end
    end
end)

FlyButtonPremium.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success and currentHasGamePass then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
        sendNotification("Latanie", "Skrypt latania załadowany!")
    else
        sendNotification("Universal Scripts", "Musisz posiadać Game Pass Premium, aby użyć tej funkcji!")
        if success then
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
        end
    end
end)

-- Logika przycisków przełączających zakładki
HomeTabButton.MouseButton1Click:Connect(function()
    showTab("Home")
end)

PremiumTabButton.MouseButton1Click:Connect(function()
    -- **KRYTYCZNA ZMIANA: Czekaj na wynik MarketplaceService:UserOwnsGamePassAsync**
    local success, currentHasGamePass = pcall(function()
        return MarketplaceService:UserOwnsGamePassAsync(player.UserId, PREMIUM_GAMEPASS_ID)
    end)

    if success then
        if currentHasGamePass then
            showTab("Premium")
            sendNotification("Universal Scripts", "Przełączono na zakładkę Premium.")
        else
            MarketplaceService:PromptGamePassPurchase(player, PREMIUM_GAMEPASS_ID)
            sendNotification("Universal Scripts", "Game Pass Premium nie znaleziony. Otwieram okno zakupu...")
        end
    else
        warn("Błąd podczas sprawdzania Game Passa po kliknięciu Premium:", currentHasGamePass)
        sendNotification("Universal Scripts", "Wystąpił błąd podczas sprawdzania Game Passa.")
    end
end)


HideButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- Obsługa zdarzenia po zakończeniu próby zakupu Game Passa
MarketplaceService.PromptGamePassPurchaseFinished:Connect(function(userId, gamePassId, wasPurchased)
    if userId == player.UserId and gamePassId == PREMIUM_GAMEPASS_ID and wasPurchased then
        hasGamePassAtStartup = true -- Aktualizujemy tylko zmienną do celów informacyjnych
        sendNotification("Universal Scripts", "Game Pass Premium kupiony pomyślnie! Ciesz się nowymi funkcjami.")
        showTab("Premium") -- Automatycznie przełącz na zakładkę Premium po zakupie
    elseif userId == player.UserId and gamePassId == PREMIUM_GAMEPASS_ID and not wasPurchased then
        sendNotification("Universal Scripts", "Anulowano zakup Game Passa Premium.")
    end
end)

-- Początkowe powiadomienie
spawn(function()
    wait(2)
    sendNotification("Universal Scripts", "Dzięki za używanie Universal Scripts! Witaj w nowej wersji!")
end)

task.wait(0.5)
