local function sendNotification(title, text)
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Icon = "rbxassetid://6022295536",
        Duration = 5
    })
end

spawn(function()
    wait(2)
    sendNotification("Universal Scripts", "Thanks for using Universal Scripts")
end)

HideButton.MouseButton1Click:Connect(function()
    Frame.Visible = not Frame.Visible
end)

Invisibiltybutton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()
    sendNotification("Invisibilty Tool", "Skrypt Invisibility załadowany!")
end)

SpeedToolbutton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://pastebin.com/raw/X7B0HPc2"))()
    sendNotification("Speed Tool", "Skrypt Speed Tool załadowany!")
end)

