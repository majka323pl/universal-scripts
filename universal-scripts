    sendNotification("Fling Tool", "Skrypt Fling załadowany!")
end)

MM2Button.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vertex-peak/vertex/refs/heads/main/loadstring"))()
