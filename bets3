-- Nowy event dla przycisku Game
GameButton.MouseButton1Click:Connect(function()
    local gameId = game.PlaceId
    local url = "https://raw.githubusercontent.com/majka323pl/universal-scripts/main/game_scripts/" .. gameId
    runScriptAndNotify(url, GameButton.Text)
end)

