-- Button connections (Executor Frame)
ExecuteButton.MouseButton1Click:Connect(function()
    local scriptContent = TextBox.Text
    local success, err = pcall(function()
        loadstring(scriptContent)()
    end)
    if success then
        sendNotification("Executor", "Skrypt wykonany pomyślnie!")
    else
        sendNotification("Executor", "Błąd wykonania skryptu: " .. tostring(err))
        warn("Błąd wykonania skryptu:", err)
    end
end)

ClearButton.MouseButton1Click:Connect(function()
    TextBox.Text = ""
    sendNotification("Executor", "Zawartość wyczyszczona.")
end)

-- Tab navigation
HomeButton.MouseButton1Click:Connect(function()
    showFrame("Home")
end)

PremiumButton.MouseButton1Click:Connect(function()
    showFrame("Premium")
end)

ExecutorButton.MouseButton1Click:Connect(function()
    showFrame("Executor")
end)

UpdateLogsButton.MouseButton1Click:Connect(function()
    showFrame("UpdateLogs")
end)

-- HideButton functionality
HideButton.MouseButton1Click:Connect(function()
    Frame.Visible = not Frame.Visible
end)

MarketplaceService.PromptGamePassPurchaseFinished:Connect(function(userId, gamePassId, wasPurchased)
    if userId == player.UserId and gamePassId == GAMEPASS_ID then
        if wasPurchased then
            sendNotification("Universal Scripts", "Wersja premium kupiona pomyślnie! Dziękujemy za zakup.")
            hasGamePass = true
            showFrame("Premium")
        else
            sendNotification("Universal Scripts", "Anulowano zakup wersji premium.")
        end
        checkGamePassStatus()
    end
end)

-- Initial setup
showFrame("Home")
checkGamePassStatus()
